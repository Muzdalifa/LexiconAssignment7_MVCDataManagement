<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.css" />
    <script src="~/lib/bootstrap/js/bootstrap.js"></script>
    <script src="~/lib/jquery/jquery.js"></script>
    <link href="~/css/StyleSheet.css" rel="stylesheet" />
   
</head>
<body>

    <nav class="navbar navbar-expand-lg navbar-light bg-light p-5">
        <a class="navbar-brand" href="#">MVC Data Management</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
                <li class="nav-item active">
                   
                    <a class="nav-link active" asp-controller="People" asp-action="Index"> People's data Assignment<span class="sr-only"></span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link " asp-controller="Ajax" asp-action="Index"> Ajax Assignment</a>
                </li>
            </ul>
        </div>
    </nav>
    <div class="row container p-5 py-3">
        @RenderBody()
    </div>



    <script src="~/js/JavaScript.js"></script>
</body>
</html>
